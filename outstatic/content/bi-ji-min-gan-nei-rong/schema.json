{
  "title": "笔记（敏感内容）元数据",
  "type": "object",
  "properties": {
    "timestamp": {
      "title": "发布时间",
      "description": "文章发布日期和时间",
      "dataType": "date",
      "fieldType": "Date"
    },
    "series": {
      "title": "系列",
      "description": "文章所属系列名称",
      "dataType": "string",
      "fieldType": "String"
    },
    "tags": {
      "title": "标签列表",
      "description": "文章标签",
      "dataType": "array",
      "fieldType": "Tags"
    },
    "description": {
      "title": "描述",
      "description": "文章简短描述/摘要",
      "dataType": "string",
      "fieldType": "Textarea"
    },
    "sensitive": {
      "title": "敏感内容",
      "description": "标记此内容是否为敏感内容",
      "dataType": "boolean",
      "fieldType": "Boolean",
      "defaultValue": true
    },
    "toc": {
      "title": "显示目录",
      "description": "是否显示文章目录",
      "dataType": "boolean",
      "fieldType": "Boolean",
      "defaultValue": false
    },
    "top": {
      "title": "优先级",
      "description": "文章排序优先级(数字越大优先级越高)",
      "dataType": "number",
      "fieldType": "Number",
      "defaultValue": 0
    },
    "draft": {
      "title": "草稿",
      "description": "是否为草稿(不显示在公开列表中)",
      "dataType": "boolean",
      "fieldType": "Boolean",
      "defaultValue": false
    }
  }
}